{
  "name": "env-switcher",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:chrome": "tsc -b && TARGET_BROWSER=chrome vite build",
    "build:firefox": "tsc -b && TARGET_BROWSER=firefox vite build",
    "watch:firefox": "TARGET_BROWSER=firefox vite build --watch",
    "package:chrome": "npm run build:chrome && cd dist && zip -r ../env-switcher-chrome.zip .",
    "package:firefox": "npm run build:firefox && cd dist && zip -r ../env-switcher-firefox.zip .",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/chrome": "^0.0.329",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-plugin-static-copy": "^3.1.0",
    "webextension-polyfill": "^0.12.0"
  }
}
